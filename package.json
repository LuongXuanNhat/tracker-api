{
  "name": "tracker-api",
  "version": "2.0.16",
  "description": "A comprehensive JavaScript/TypeScript library for user behavior tracking and analytics",
  "main": "lib/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "browser/tracker-api-bundle.js",
  "scripts": {
    "build": "npm run build:node && npm run build:browser",
    "build:node": "tsc",
    "build:browser": "node scripts/build-browser-simple.js",
    "build:watch": "tsc --watch",
    "clean": "rimraf dist browser",
    "prepare": "npm run clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ts": "node -r ts-node/register examples/usage-examples.ts",
    "test:js": "node examples/javascript-usage.js",
    "test:browser": "echo \"Open examples/quick-test.html for quick test, or examples/browser-demo.html for full demo\"",
    "dev": "npm run build:watch",
    "lint": "tsc --noEmit"
  },
  "keywords": [
    "tracking",
    "analytics",
    "user-behavior",
    "web-analytics",
    "typescript",
    "javascript",
    "customer-management",
    "website-analytics",
    "real-time-analytics",
    "api-client"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "jest": "^30.0.5",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "axios": "^1.0.0"
  },
  "files": [
    "dist/",
    "browser/",
    "lib/",
    "src/",
    "examples/",
    "scripts/",
    "*.d.ts",
    "README.md",
    "README-NEW.md",
    "MIGRATION.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tracker-api.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/tracker-api/issues"
  },
  "homepage": "https://github.com/yourusername/tracker-api#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./lib/index.js",
      "import": "./dist/index.js",
      "browser": "./dist/browser/index.js"
    },
    "./lib": {
      "require": "./lib/index.js"
    },
    "./dist": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./browser": {
      "browser": "./browser/tracker-api-bundle.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
